{
  "name": "nest-graphql-template-arsen",
  "version": "0.0.1",
  "description": "",
  "author": "arsen",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "fix:migration": "npx sequelize-cli db:migrate:undo:all & npx sequelize-cli db:migrate & npx sequelize-cli db:seed:all",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "npx sequelize-cli migration:create --name",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "seed:run": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@langchain/community": "^0.2.32",
    "@langchain/core": "^0.2.31",
    "@langchain/ollama": "^0.0.4",
    "@nestjs/apollo": "^12.2.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/throttler": "^6.2.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/memory-cache": "^0.2.6",
    "@types/sequelize": "^4.28.20",
    "axios": "^1.7.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "express-session": "^1.18.0",
    "graphql": "^16.9.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-query-complexity": "^1.0.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint",
      "options": {
        "sort": "alphabetical",
        "groupBy": [
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ]
      }
    }
  }
}
